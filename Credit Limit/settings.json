{
    "al.enableCodeActions": true,
    "al.enableCodeAnalysis": false,
    "al.codeAnalyzers": [
        "${CodeCop}",
        //"${PerTenantExtensionCop}", //not for ISV
        "${AppSourceCop}",
        "${UICop}" 
    ],
    "CRS.ObjectNamePrefix": "Test",
    "CRS.FileNamePattern": "<ObjectTypeShort><ObjectId>.<ObjectNameShort>.al",
    "CRS.FileNamePatternExtensions": "<ObjectType>.<BaseId>,<BaseNameShort>,.al",
    "CRS.OnSaveAlFileAction": "Rename",
    "git.enableSmartCommit": true,
    "editor.maxTokenizationLineLength": 50000,
    "gitignore.cacheExpirationInterval": 3600,
    "al.ruleSetPath": "./ruleset.json"
}